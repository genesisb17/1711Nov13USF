package Day2;

public class MergeSort 
{
	int righti;
	int lefti;
	int[] array;
	int[] mergeSort(int righti,int lefti,int...array)
	{
		int temp;
	     for(int i=righti+1; i < lefti; i++){  
	         for(int j=righti; j < i; j++){  
	                  if(array[j-1] > array[j]){  
	                         //swap elements  
	                         temp = array[j-1];  
	                         array[j-1] = array[j];  
	                         array[j] = temp;  
	                 }                  
	         }  
	     }
	     return array;
	}
	int[] combine(int[] arr1,int[] arr2)
	{
		
		int l = arr1.length+arr2.length;
		int [] sol = new int[l];
		return sol;
		
	}
}
