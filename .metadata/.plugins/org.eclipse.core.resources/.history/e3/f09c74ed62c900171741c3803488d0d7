package Day2;

public class searchandsorts 
{
/*Implement the following
 * binary search
 * breadth the first search
 * dpth first search 
 * bubble sort
 * merge sort
 * insertion sort
 */
	public static void main(String[] args)
	{
		int arr[]= {1,2,3,4,5,6};
		bubbleSort(arr);
	}
 void binarySearch(int key,int... nums)
 {
	 int size = nums.length;
	 int low = 0;
	 int high = size-1;
	 while(high >=low){
		 int middle=(low+high)/2;
	 }
 }
 static void bubbleSort(int... arr)
 {
	 int n = arr.length;
	 int temp;
     for(int i=0; i < n; i++){  
         for(int j=1; j < (n-i); j++){  
                  if(arr[j-1] > arr[j]){  
                         //swap elements  
                         temp = arr[j-1];  
                         arr[j-1] = arr[j];  
                         arr[j] = temp;  
                 }  
                  
         }  
 }
  
     for(int i=0; i < n; i++){  
         
        	 System.out.println(arr[i]);
                    
 } }
}
