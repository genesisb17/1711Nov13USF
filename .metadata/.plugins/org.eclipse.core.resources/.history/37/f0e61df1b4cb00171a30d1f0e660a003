package day5.LabAssignment;

public class Problem1 
{

	int[] b = {1,0,5,6,3,2,3,7,9,8,4};
	int temp;
	for(int i=0;i<b.length;i++)
	{
		for(int j=1; j < b.length;j++)
		{
		     if(b[j-1] > b[j]){  
                 //swap elements  
                 temp = b[j-1];  
                 b[j-1] = b[j];  
                 b[j] = temp;  
         }    
		}
	}
	for(int i=0;i<b.length;i++)
	{
		System.out.println(b[i]);
	}

}
