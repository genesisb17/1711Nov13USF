package day4;

public class Even_checker 
{
	boolean test(int num)
	{
		String s;
		String[] s1;
		String[] s2 = {"2","4","6","8","0"};
		s = Integer.toString(num);
		s1 = s.split("");
		System.out.println(s1[s1.length-1]);
		for(int i = 0;i<s1.length;i++)
		{
			
			if(s1[s1.length-1].equals(s2[i]))
			{
				return true;
			}
		}
		
		return false;
	}
}
