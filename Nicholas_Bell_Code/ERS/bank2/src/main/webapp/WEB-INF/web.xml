<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name>bank</display-name>
  <welcome-file-list>
    <welcome-file>landing.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>hello</servlet-name>
    <servlet-class>com.bank.servlets.HelloServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>hello</servlet-name>
    <url-pattern>/hello</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>GetHomeViewServlet</display-name>
    <servlet-name>GetHomeViewServlet</servlet-name>
    <servlet-class>com.bank.servlets.GetHomeViewServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>GetHomeViewServlet</servlet-name>
    <url-pattern>/GetHomeViewServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>GetRequestHelper</display-name>
    <servlet-name>GetRequestHelper</servlet-name>
    <servlet-class>com.bank.servlets.GetRequestHelper</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>GetRequestHelper</servlet-name>
    <url-pattern>/GetRequestHelper</url-pattern>
  </servlet-mapping>
</web-app>