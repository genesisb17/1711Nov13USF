--STORED PROC

CREATE OR REPLACE PROCEDURE helloWorld
AS
BEGIN
dbms_output.put_line('Hello database world!');
END;
/

execute helloWorld;

select * from artist
WHERE artistid = 4;

CREATE SEQUENCE ACC_SEQ
START WITH 500
INCREMENT BY 1;
/
CREATE OR REPLACE TRIGGER ART_TRIGGER
BEFORE INSERT ON ARTIST
FOR EACH ROW
BEGIN 
SELECT ART_SEQ.NEXTVAL INTO :NEW.ARTISTID FROM DUAL;
END;
/
COMMIT;

insert into artist (Name) values ('Beyonce');

select * from artist;

UPDATE artist SET name = 'bob' WHERE ARTISTID = 1;
