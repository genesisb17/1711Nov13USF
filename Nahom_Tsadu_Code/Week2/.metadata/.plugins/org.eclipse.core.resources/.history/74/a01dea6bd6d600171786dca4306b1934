function formatAttribute(attribute){
	return "#" + attribute;
}

function formatTabAttribute(attribute){
	return "#" + attribute + "Tab";
}

function formatButtonAttribute(attribute){
	return "#" + attribute + "-button";
}

function formatModalAttribute(attribute){
	return "#" + attribute + "Modal";
}

function formatButtonIconAttribute(attribute){
	return "#" + attribute + "-button-icon";
}