function initMaps(){
	map = getAccordionMap();
	diagramMap = getDiagramMap();
	diagramFormat = getDiagramFormatMap();
	iconMap = getIconMap();
}

function getAccordionMap(){
	return accordionMap;
}

function getDiagramMap(){
	return diagramMap;
}

function getDiagramFormatMap(){
	return diagramFormatMap;
}

function getIconMap(){
	return iconMap;
}