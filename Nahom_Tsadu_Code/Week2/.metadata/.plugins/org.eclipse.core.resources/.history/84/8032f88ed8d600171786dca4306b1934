var map;
var diagramMap;
var diagramFormat;
var iconMap;

var mainModal;
var modalElements = [];
var introductionElements = [];
var overloadingElements = [];
var overridingElements = [];
var coercionElements = [];

$(document).ready(function(){

	initialize();
	console.log(mainModal);
	$(mainModal).on('shown.bs.modal', function () {console.log(mainModal); setModalFocus();})
	$(stringifyElements(modalElements)).click(function(){setModalDiagram(this);});
	$(stringifyElements(introductionElements)).click(function(){setIntroductionView();});
	$(stringifyElements(overloadingElements)).click(function(){setOverloadingView();});
	$(stringifyElements(overridingElements)).click(function(){setOverridingView();});
	$(stringifyElements(coercionElements)).click(function(){setCoercionView();});
});

function initialize(){
	initMaps();
	initModals();
	initIntroductionElements();
	initOverloadingElements();
	initOverridingElements();
	initCoercionElements();
	
	//modalElements = [introductionModalContent, overloadingModalContent, overridingModalContent, coercionModalContent];
	
	
	

	
}

function stringifyElements(elements){
	return elements.toString();
}




