
public class DoinStrings {

	public static void main(String[] args) {
		StringBuilder build = new StringBuilder("Holy crap it's a string!");
		System.out.println(build);
		build.append(true);
		System.out.println(build);
		build.reverse();
		System.out.println(build);
		build.replace(2, 5, "Yee");
		System.out.println(build);
		

	}

}
